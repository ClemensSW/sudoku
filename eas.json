{
  "cli": {
    "version": ">= 15.0.13",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "applicationIdSuffix": ".dev"
      },
      "ios": {
        "bundleIdentifierSuffix": ".dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "applicationIdSuffix": ".preview"
      },
      "ios": {
        "bundleIdentifierSuffix": ".preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
