{
  "expo": {
    "name": "Sudoku Duo",
    "slug": "sudoku-duo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sudokuduo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router"
    ],
    "ios": {
      "supportsTablet": true,
      "statusBarHidden": true,
      "bundleIdentifier": "com.yourdomain.sudoku"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E2233"
      },
      "softwareKeyboardLayoutMode": "pan",
      "statusBar": {
        "hidden": true,
        "translucent": true,
        "backgroundColor": "#00000000"
      },
      "package": "de.playfusiongate.sudokuduo",
      "versionCode": 1,
      "permissions": [],
      "intentFilters": [
        {
          "action": "MAIN",
          "category": [
            "LAUNCHER",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "data": {
            "scheme": "mailto"
          },
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#1E2233"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b6add40-870a-4135-9786-bc0120501791"
      }
    }
  }
}
